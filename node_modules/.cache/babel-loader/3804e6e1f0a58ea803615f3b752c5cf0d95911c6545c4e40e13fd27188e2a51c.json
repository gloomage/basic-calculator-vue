/src/components/CalculatorBlock.vue"],"sourcesContent":["<template>\n    <div class=\"container\">\n        <div class=\"box\">\n            <div class=\"titulo\">\n                <h2>CALCULATOR</h2>\n            </div>\n            <div class=\"columns\">\n\n                <div class=\"col-80\">\n                    <div class=\"row\">\n                        <label> VALUE 1:\n                            <input type=\"number\" v-model=\"value1\" />\n                        </label>\n                    </div>\n                    <div class=\"row\">\n                        <label> VALUE 2:\n                            <input type=\"number\" v-model=\"value2\" />\n                        </label>\n                    </div>\n                    <div class=\"row\">\n                        <label> RESULT:\n                            <input type=\"number\" v-model=\"result\" />\n                        </label>\n                    </div>\n                </div>\n\n                <div class=\"col-20\">\n                    <div class=\"row\">\n                        <button style=\"color: blue;\" v-on:click=\"adicao\">\n                            SOMAR\n                        </button>\n                    </div>\n                    <div class=\"row\">\n                        <button style=\"color: blue;\" v-on:click=\"subtracao\">\n                            SUBTRAIR\n                        </button>\n                    </div>\n                    <div class=\"row\">\n                        <button style=\"color: blue;\" v-on:click=\"multiplicacao\">\n                            MULTIPLICAR\n                        </button>\n                    </div>\n                    <div class=\"row\">\n                        <button style=\"color: blue;\" v-on:click=\"divisao\">\n                            DIVIDIR\n                        </button>\n                    </div>\n                    <div class=\"row\">\n                        <button style=\"color: red;\" v-on:click=\"clear\">\n                            LIMPAR\n                        </button>\n                    </div> \n                </div>\n\n            </div>\n        </div>\n    </div>\n</template>\n\n<script>\nimport { defineComponent } from 'vue';\n\nexport default defineComponent({\n    name: 'CalculatorBlock',\n    data () {\n        return {\n            value1: null,\n            value2: null,\n            result: null,\n        }\n    },\n    methods: {\n        adicao () {\n            if(this.validation()){\n                this.result = this.value1 + this.value2;\n            }\n        },\n        subtracao () {\n            this.result = this.value1 - this.value2;\n        },\n        multiplicacao () {\n            this.result = this.value1 * this.value2;\n        },\n        divisao () {\n            this.result = this.value1 / this.value2;\n        },\n        clear () {\n            this.value1 = null;\n            this.value2 = null;\n            this.result = null;\n        },\n        validation () {\n            if(this.value1 == null || this.value2 == null){\n                // this.errors.push('Complete os campos: (Obrigatório)');\n                return false;\n            }\n            return true;\n        }\n    }\n})\n</script>\n\n<style>\ninput {\n  display: block;\n  height: 25px;\n  border-radius: 2px;\n  border-color: darkgray;\n  width: 100%;\n}\nh2 {\n    margin-left: 30px;\n    color: white;\n}\nbutton {\n    min-height: 30px;\n    min-width: 100px;\n    border-color: darkgray;\n    border-style: solid;\n    border-radius: 5px;\n}\n.box {\n    width: 500px;\n    height: 350px;\n    background: #fff;\n    border-color: darkgray;\n    border-style: solid;\n    border-radius: 10px;\n}\n.titulo {\n    background: gray;\n    margin: 5px;\n    border-color: darkgray;\n    border-style: solid;\n    border-radius: 10px;\n}\n.container{\n    width: 100vw;\n    height: 100vh;\n    background: #6C7A89;\n    display: flex;\n    flex-direction: row;\n    justify-content: center;\n    align-items: center\n}\n\n.columns {    \n    margin: 5px;\n    display: flex;\n    flex-direction: row;\n    flex-flow: row wrap;\n    align-items: center;\n}\n\n.col-80 {\n    width: 70%;\n    height: 250px;\n    text-align: left;\n}\n.col-20 {\n    width: 30%;\n    height: 250px;\n    text-align: center;\n}\n.row{\n    min-height: 30px;\n    margin-top: 20px;\n\n}\n</style>"]},"metadata":{},"sourceType":"module","externalDependencies":[]}